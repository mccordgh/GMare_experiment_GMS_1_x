        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Collect Exp and Level Up

// get exp_worth from other
exp_worth = other.exp_worth;

// Destroy xp bubble
with (other) instance_destroy();

// add to player experience
obj_player_stats.expr += exp_worth;

// Add to player stats exp
with (obj_player_stats) {
    
    // Check for level up
    if (expr &gt;= max_expr) {
           
        // Give stat bonuses for level up
        level += 1;
        expr -= max_expr;
        max_expr = floor(max_expr * 1.5);
        hp += 2;
        max_hp += 2;
        stamina += 2;
        max_stamina += 2;
        attack += 1;
    }
}
</string>
          </argument>
        </arguments>
      </action>

            <string>/// Draw Menu Text

// Get screen center
var xx = display_get_gui_width() / 2;
var yy = display_get_gui_height() / 2;

// center the horizontal alignment
draw_set_halign(fa_center);

// draw menu title
draw_text(xx, yy - 64, title);

// draw each menu option
for (var i = 0; i &lt; array_length_1d(option); i++) {
    // draw current selected menu item white, and the rest gray
    if (i == menu_index) {
        draw_set_color(c_white);
    } else {
        draw_set_color(c_gray);
    }
    draw_text(xx, yy + (i * 32), option[i]);
}

draw_set_color(c_white);
draw_set_halign(fa_left);
</string>
